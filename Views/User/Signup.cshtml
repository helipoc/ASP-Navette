@if (ViewBag.Error != null)
{
    <div class="alert alert-danger">
    @ViewBag.Error
</div>
}
@if (ViewBag.Success != null)
{
    <div class="alert alert-success">
    @ViewBag.Success
</div>
}
@if (ViewBag.Warn != null)
{
    <div class="alert alert-warning">
    @ViewBag.Warn
</div>
}

